{
  "name": "danieldreier-resource_facts",
  "version": "0.1.1",
  "author": "danieldreier",
  "summary": "Provide current state of all puppet resources as a huge structured fact",
  "license": "Apache-2.0",
  "source": "https://github.com/danieldreier/puppet-resource_facts",
  "project_page": "https://github.com/danieldreier/puppet-resource_facts",
  "issues_url": "https://github.com/danieldreier/puppet-resource_facts/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.0.0 <5.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 1.0.0 <2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "4.x"
    }
  ]
}
